{
  "extends": "@vue/tsconfig/tsconfig.json",
  "include": [
    "**/serviceWorker.ts",
    "**/service-worker.ts",
    "**/serviceworker.ts",
    "**/sw.ts",
    "global.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "noEmit": false,
    "lib": ["ES2022", "WebWorker"],
    "target": "ES2022",
    "module": "ES2022",
    "types": [],
    "strict": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": true
  }
}
